{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-azure-clinical-dw-dev"
		},
		"ACDWMongoDbLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ACDWMongoDbLinkedService'"
		},
		"AzureDataLakeStorageLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageLinkedService'"
		},
		"ACDWMongoDbLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azure_clinical_data_warehouse"
		},
		"ACDWSqlServerLinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-1PK2BM8\\SQLEXPRESS"
		},
		"ACDWSqlServerLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azure_clinical_data_warehouse"
		},
		"ACDWSqlServerLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mercy"
		},
		"AzureDataLakeStorageLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sazureclinicaldw.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvAzureClinicalDW.vault.azure.net/"
		},
		"AzureKeyVaultLinkedService_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvAzureClinicalDW.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-azure-clinical-dw-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageLinkedService_accountKey": {
						"value": "[parameters('AzureDataLakeStorageLinkedService_accountKey')]"
					},
					"ACDWMongoDbLinkedService_connectionString": {
						"value": "[parameters('ACDWMongoDbLinkedService_connectionString')]"
					},
					"AzureDataLakeStorageLinkedService_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageLinkedService_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureKeyVaultLinkedService_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultLinkedService_properties_typeProperties_baseUrl')]"
					},
					"ACDWMongoDbLinkedService_properties_typeProperties_database": {
						"value": "[parameters('ACDWMongoDbLinkedService_properties_typeProperties_database')]"
					},
					"ACDWSqlServerLinkedService_properties_typeProperties_server": {
						"value": "[parameters('ACDWSqlServerLinkedService_properties_typeProperties_server')]"
					},
					"ACDWSqlServerLinkedService_properties_typeProperties_database": {
						"value": "[parameters('ACDWSqlServerLinkedService_properties_typeProperties_database')]"
					},
					"ACDWSqlServerLinkedService_properties_typeProperties_userName": {
						"value": "[parameters('ACDWSqlServerLinkedService_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}